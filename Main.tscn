[sub_resource type="GDScript" id=10]
script/source = "extends Spatial

## todo: make this a setget for tool compatibility? check if it works first try
export(Dictionary) var properties

var default_actor = \"Enemy\"

func _init() -> void:
	add_to_group(\"SpawnPoints\")
	if \"default_actor\" in properties:
		default_actor = \"default_actor\"

func _ready():
	if \"spawn_on_start\" in properties:
		if \"spawn_on_start\":
			spawn()
	spawn()

func spawn(actor_type: String = default_actor) -> void:
	if not is_inside_tree():
		yield(self, \"ready\")
	var path = \"\"
	match actor_type:
		\"Player\":
			path = \"res://actors/player.tscn\"
		\"Enemy\":
			path = \"res://actors/enemy.tscn\"
	var actor: Spatial = load(path).instance()
	actor.set_as_toplevel(true)
	add_child(actor)
"

